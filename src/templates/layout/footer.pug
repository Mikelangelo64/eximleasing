footer.footer(class=`${isFooterNearSeo ? 'near-seo__below' : ''}`)
  ._container
    .footer__wrapper
      .footer__main
        .footer__item
          ul.footer-contacts.contact-list
            each item, index in footerContacts
              li(class=`${item.additional ? 'additional' : ''}`)
                a.footer-contacts__link.contact-link(
                  href=`${item.href}`,
                  target=`${item.isTargetBlank === 'true' ? '_blank' : '_self'}`
                )
                  span.contact-link__label!= item.image

                  span.contact-link__text= item.title

                if item.additional
                  a.footer-contacts__link.contact-link.additional(
                    href=`${item.additional.href}`,
                    target=`${item.isTargetBlank === 'true' ? '_blank' : '_self'}`
                  )
                    span.contact-link__text= item.additional.title

      .footer-bottom
        .footer-bottom__container
          .footer-bottom__item
            p.footer__group Група компаній Caspit
              |
              a(href='https://caspit.group', target='_blank', rel='noreferrer') https://caspit.group

            p.footer__copyright © 2023 Ексімлізинг

          .footer-bottom__item
            a.footer-bottom__link(href='#') Політика конфіденційності

          .footer-bottom__item
            a.develop(
              href='https://impulse-design.com.ua/',
              target='_blank',
              rel='noreferrer'
            )
              span.develop__text Developed By

              span.develop__img
                img(src=require('@/assets/img/impulse.svg'), alt='impulse')
